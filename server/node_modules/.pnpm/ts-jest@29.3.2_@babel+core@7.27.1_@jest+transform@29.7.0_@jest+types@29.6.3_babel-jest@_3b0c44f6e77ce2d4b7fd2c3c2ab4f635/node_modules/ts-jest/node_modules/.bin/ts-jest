#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/sites/chat-x/server/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_3b0c44f6e77ce2d4b7fd2c3c2ab4f635/node_modules/ts-jest/node_modules:/data/data/com.termux/files/home/sites/chat-x/server/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_3b0c44f6e77ce2d4b7fd2c3c2ab4f635/node_modules:/data/data/com.termux/files/home/sites/chat-x/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/sites/chat-x/server/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_3b0c44f6e77ce2d4b7fd2c3c2ab4f635/node_modules/ts-jest/node_modules:/data/data/com.termux/files/home/sites/chat-x/server/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_3b0c44f6e77ce2d4b7fd2c3c2ab4f635/node_modules:/data/data/com.termux/files/home/sites/chat-x/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
